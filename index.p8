pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
px = 60
py = 60
ex = rnd(128)
ey = rnd(128)
function _draw()
		cls(1)
		
		spr(1,px,py)
		
		if(btn(0)) px-=2
		if(btn(1)) px+=2
		if(btn(2)) py-=2
		if(btn(3)) py+=2
		
		spr(2,ex,ey)
		if(px == ex and py == ey)then
				ex = rnd(128)
				ey = rnd(128)
		end
end
